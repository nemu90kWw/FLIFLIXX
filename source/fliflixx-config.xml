<flex-config>
  <use-network>false</use-network>
  
  <default-size>
    <width>640</width>
    <height>400</height>
  </default-size>
  <default-background-color>0x000000</default-background-color>
  <default-frame-rate>60</default-frame-rate>
  
  <default-script-limits>
    <max-recursion-depth>65535</max-recursion-depth>
    <max-execution-time>7</max-execution-time>
  </default-script-limits>
  
  <compiler>
    <library-path append="true">
		<path-element>libs</path-element>
	</library-path>
    
    <warn-duplicate-variable-def>false</warn-duplicate-variable-def>
    <warn-deprecated-function-error>true</warn-deprecated-function-error>
    <warn-deprecated-property-error>true</warn-deprecated-property-error>
  </compiler>
  
  <metadata>
    <title>FLIFLIXX</title>
    <description></description>
    <publisher>90k-games</publisher>
    <creator>ねむ◆nemu90kWw.</creator>
    <language>ja-JP</language>
  </metadata>
</flex-config>
